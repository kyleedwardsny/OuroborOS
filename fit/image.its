/dts-v1/;

/ {
	description = "OuroborOS boot image";
	#address-cells = <1>;

	images {
		ou_ikernel: kernel@1 {
			hash@1 {
				algo = "crc32";
			};
			hash@2 {
				algo = "sha1";
			};
		};

		ou_fdt: fdt@1 {
			hash@1 {
				algo = "crc32";
			};
			hash@2 {
				algo = "sha1";
			};
		};
	};

	configurations {
		default = "config@1";
		config@1 {
			description = "Boot OuroborOS";
			kernel = "kernel@1";
			fdt = "fdt@1";
		};
	};
};

/include/ "fit/ikernel.itsi"
/include/ "fit/fdt.itsi"
